         PROCEDURE MaFenêtre(gnValeur est un entier = 0, gnValeurMaximale est un entier = 10 000)
         
         
         CONSTANTE
         	TYPE_ROND = 1
         	TYPE_ANNEAU = 2
         FIN
         
         // Angle de départ de la jauge
         gnAngleDépart est un entier = 90 
         // Type d'affichage (au choix TYPE_ANNEAU ou TYPE_ROND)
         gnType est un entier = TYPE_ANNEAU
         // Couleur de la jauge (si couleur unie)
         gnCouleurJauge est un entier = RVB(41, 128, 185) 
         // Couleur de la zone de décor (rond ou anneau selon le type)
         gnCouleurDécor est un entier =  gnType=TYPE_ROND ? RVB(52, 73, 94) 	SINON  RVB(239, 246, 255) 
         // Couleur du texte (rond ou anneau selon le type)
         gnCouleurTexte est un entier =  gnType=TYPE_ROND ? RVB(226, 232, 233) SINON  LIB_Valeur..couleur
         // Vrai pour afficher le texte au centre, Faux autrement
         gbAvecTexte est un booléen = Vrai 
         // Sens horaire ou non
         gbSensHoraire est un booléen = Vrai
         
         
         // Dessin initial à 0 (optionnel)
         DessineJauge(gnValeur,gnValeurMaximale)
         
